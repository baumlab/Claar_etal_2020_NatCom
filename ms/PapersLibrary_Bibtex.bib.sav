@book{Brodie:2007vm,
author = {Brodie, Jedediah Farrell},
title = {{Effects of Seed Dispersal by Gibbons, Sambar, and Muntjac on Choerospondias Axillaris Demography, and the Disruption of this Mutualism by Wildlife Poaching}},
publisher = {ProQuest},
year = {2007},
