journal = {Conservation Biology},
year = {2007},
volume = {21},
number = {3},
pages = {741--751},
month = jun,
affiliation = {CRBPO, UMR-5173-Mus{\'e}um National d'Histoire Naturelle, 55 rue Buffon, CP 51, 75005 Paris, France.},
