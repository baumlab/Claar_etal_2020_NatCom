file = {{He_2015_Microbiome.pdf:/Users/danielle/Library/Application Support/Papers2/He/2015/Articles/He_2015_Microbiome.pdf:application/pdf}},
uri = {\url{papers2://publication/doi/10.1186/s40168-015-0081-x}}
}

